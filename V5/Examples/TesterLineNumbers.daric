100 DEF PROCFail()
110 	COLOUR RED
120 	PRINT  "!! FAILED TEST !!"
130 	END
140 ENDPROC
150 :
200 DEF PROCPass()
210 	COLOUR GREEN
220 	PRINT "PASS"
230 	COLOUR WHITE
240 ENDPROC
250 :
300 PRINT "::GOTO & GOSUB::"
310 PRINT "GOTO               = ";
320 GOTO 340
330 PROCFail()
340 PROCPass()
350 PRINT "GOSUB              = ";
360 GOSUB%=0
370 GOSUB 1000
380 IF GOSUB%=1 THEN PROCPass() ELSE PROCFail()
390 RETURN
1000 GOSUB%=1:RETURN

